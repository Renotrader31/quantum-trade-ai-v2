{
  "version": 2,
  "name": "quantum-trade-ai-v2-enhanced",
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "/NUCLEAR_DEPLOYMENT_ENHANCED.html"
    },
    {
      "source": "/app",
      "destination": "/NUCLEAR_DEPLOYMENT_ENHANCED.html"
    },
    {
      "source": "/trading",
      "destination": "/NUCLEAR_DEPLOYMENT_ENHANCED.html"
    }
  ],
  "headers": [
    {
      "source": "/NUCLEAR_DEPLOYMENT_ENHANCED.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Cache-Control", 
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}